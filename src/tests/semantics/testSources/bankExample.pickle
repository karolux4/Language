cannon{
	int shared balance = 20000;
	fork{
		int a=10;
		while(a>0){
			sync{
				balance = balance-200;
			}
			a=a-1;
		}
	}
	
	fork{
		int a=10;
		while(a>0){
			sync{
				balance = balance+300;
			}
			a=a-1;
		}
	}
	
	fork{
		int a=10;
		while(a>0){
			sync{
				balance = balance-1000;
			}
			a=a-1;
		}
	}
	
	join;
	print(balance);
}