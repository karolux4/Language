/* February has 29 days if it is a leap year, otherwise 28 days*/
/* Leap year is a year that is a multiple of 4, but if it is also the multiple of 100,
   then it also has to be a multiple of 400, otherwise it is not a leap year*/
pickle days(int year){
	int div1 = year/4;
	int div2 = year/100;
	int div3 = year/400;
	if(div1*4==year){
		if(div2*100==year){
			if(div3*400==year){
				print(29);
			}
			else{
				print(28);
			}
		}
		else{
			print(29);
		}
	}
	else{
		print(28);
	}
}
cannon{
	days(2012);
	days(1055);
	days(2000);
	days(1700);
	days(1040);
}