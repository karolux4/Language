pickle positiveArray(int a[6]){
	int i=0;
	while(i<=5){
		if(a[i]>0){
			print(a[i]);
		}
		i=i+1;
	}
}

cannon {
	int shared total = 0;
	int shared a[6] = [4,-2,1,-3,10,9];
	fork {
		int i=0;
		int sum = 0;
		while(i<3){
			if(a[i]>0){
				sum=sum+a[i];
			}
			i=i+1;
		}
		sync {
			total = total + sum;
		}
	}	
	fork {
		int i=3;
		int sum =0;
		while(i<6){
			if(a[i]>0){
				sum=sum+a[i];
			}
			i=i+1;
		}
		sync {
			total = total + sum;
		}
	}
	join;
	positiveArray(a);
	print(total);
}